    "format": "prettier --write '**/*.{ts,md}'"
  },
  "license": "MIT",
  "imports": {
    "punycode": "npm:punycode@^2.3.1",
    "schemes": "npm:schemes@^1.4.0",
    "smtp-address-parser": "npm:smtp-address-parser@^1.0.3",
    "uri-js": "npm:uri-js@^4.4.1",
    "ajv": "npm:ajv@^8.17.1",
    "prettier": "npm:prettier@^2.3.2"
  }
}
